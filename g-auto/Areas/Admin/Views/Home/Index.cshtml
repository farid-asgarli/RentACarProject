
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

@model g_auto.ViewModels.Dashboard

<div class="grid grid-cols-12 gap-6">
    <div class="col-span-12 xxl:col-span-9 grid grid-cols-12 gap-6">
        <!-- BEGIN: General Report -->
        <div class="col-span-12 mt-8">
            <div class="intro-y flex items-center h-10">
                <h2 class="text-lg font-medium truncate mr-5">
                    General Report
                </h2>
                <a href="" class="ml-auto flex text-theme-1 dark:text-theme-10 rld-data"> <i data-feather="refresh-ccw" class="w-4 h-4 mr-3"></i> Reload Data </a>
            </div>
            <div class="grid grid-cols-12 gap-6 mt-5">
                <div class="col-span-12 sm:col-span-6 xl:col-span-3 intro-y">
                    <a href="@Url.Action("Index","Sale")">
                        <div class="report-box zoom-in">
                            <div class="box p-5">
                                <div class="flex">
                                    <i data-feather="shopping-cart" class="report-box__icon text-theme-10"></i>
                                    <div class="ml-auto">
                                        <div class="report-box__indicator bg-theme-9 tooltip cursor-pointer" title="33% Higher than last month"> 33% <i data-feather="chevron-up" class="w-4 h-4"></i> </div>
                                    </div>
                                </div>
                                <div class="text-3xl font-bold leading-8 mt-6">@Model.Sales.Where(c => c.isFinished).Count()</div>
                                <div class="text-base text-gray-600 mt-1">Item Sales</div>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="col-span-12 sm:col-span-6 xl:col-span-3 intro-y">
                    <a href="@Url.Action("Index","Sale")">

                        <div class="report-box zoom-in">
                            <div class="box p-5">
                                <div class="flex">
                                    <i data-feather="credit-card" class="report-box__icon text-theme-11"></i>
                                    <div class="ml-auto">
                                        <div class="report-box__indicator bg-theme-6 tooltip cursor-pointer" title="2% Lower than last month"> 2% <i data-feather="chevron-down" class="w-4 h-4"></i> </div>
                                    </div>
                                </div>
                                <div class="text-3xl font-bold leading-8 mt-6">@Model.Sales.Where(c => c.isFinished == false).Count()</div>
                                <div class="text-base text-gray-600 mt-1">New Orders</div>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="col-span-12 sm:col-span-6 xl:col-span-3 intro-y">
                    <a href="@Url.Action("Index","Reservation")">
                        <div class="report-box zoom-in">
                            <div class="box p-5">
                                <div class="flex">
                                    <i data-feather="trello" class="report-box__icon text-theme-12"></i>
                                    <div class="ml-auto">
                                        <div class="report-box__indicator bg-theme-9 tooltip cursor-pointer" title="12% Higher than last month"> 12% <i data-feather="chevron-up" class="w-4 h-4"></i> </div>
                                    </div>
                                </div>
                                <div class="text-3xl font-bold leading-8 mt-6">@Model.Reservations.Count()</div>
                                <div class="text-base text-gray-600 mt-1">Total Reservations</div>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="col-span-12 sm:col-span-6 xl:col-span-3 intro-y">
                    <a href="@Url.Action("Index","Appointment")">
                        <div class="report-box zoom-in">
                            <div class="box p-5">
                                <div class="flex">
                                    <i data-feather="calendar" class="report-box__icon text-theme-9"></i>
                                    <div class="ml-auto">
                                        <div class="report-box__indicator bg-theme-9 tooltip cursor-pointer" title="22% Higher than last month"> 22% <i data-feather="chevron-up" class="w-4 h-4"></i> </div>
                                    </div>
                                </div>
                                <div class="text-3xl font-bold leading-8 mt-6">@Model.Appointments.Count(c => c.isFinished = false)</div>
                                <div class="text-base text-gray-600 mt-1">New Appointments</div>
                            </div>
                        </div>
                    </a>
                </div>
            </div>
        </div>
        <!-- END: General Report -->
        <!-- BEGIN: Sales Report -->
        <div class="col-span-12 lg:col-span-9 mt-8">
            <div class="intro-y block sm:flex items-center h-10">
                <h2 class="text-lg font-medium truncate mr-5">
                    Latest Reservations
                </h2>
            </div>
            <div class="intro-y box p-5 mt-12 sm:mt-5">
                <table class="table table-report table-report--bordered display w-full">
                    <thead>
                        <tr>

                            <th class="border-b-2 text-left whitespace-no-wrap">MODEL NAME</th>
                            <th class="border-b-2 text-center whitespace-no-wrap">DATE</th>
                            <th class="border-b-2 text-center whitespace-no-wrap">TOTAL</th>
                            <th class="border-b-2 text-center whitespace-no-wrap">STATUS</th>
                            <th class="border-b-2 text-center whitespace-no-wrap">ACTIONS</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.Reservations.FirstOrDefault() != null)
                        {
                            foreach (g_auto.Models.Reservation item in Model.Reservations.OrderByDescending(c => c.PostDate).Take(5))
                            {
                                <tr>
                                    <td class="border-b">
                                        @if (item.Model != null)
                                        {
                                            <div class="font-medium whitespace-no-wrap">@item.Model.Name</div>
                                            <div class="text-gray-600 text-xs whitespace-no-wrap">@item.Model.Brand.Name</div>
                                        }
                                    </td>

                                    <td class="text-center border-b">@item.PostDate.ToString("MMM d")</td>
                                    @*<td class="text-center border-b">@item.Amount.ToString("0")</td>*@
                                    <td class="text-center border-b">$@item.Price.ToString("0.00")</td>
                                    <td class="w-40 border-b ">
                                        @if (item.isCancelled)
                                        {
                                            <div class="flex items-center sm:justify-center text-theme-6"> <i data-feather="x-circle" class="w-4 h-4 mr-2"></i> Cancelled </div>
                                        }
                                        else if (item.isActive)
                                        {
                                            <div class="flex items-center sm:justify-center text-theme-10"> <i data-feather="activity" class="w-4 h-4 mr-2"></i> Active </div>
                                        }
                                        else if (item.isFinished)
                                        {
                                            <div class="flex items-center sm:justify-center text-theme-9"> <i data-feather="check" class="w-4 h-4 mr-2"></i> Finished </div>
                                        }
                                        else
                                        {
                                            <div class="flex items-center sm:justify-center text-theme-11"> <i data-feather="alert-circle" class="w-4 h-4 mr-2"></i> Pending </div>
                                        }

                                    </td>
                                    <td class="border-b w-5 status_container">
                                        <div class="flex sm:justify-center items-center">
                                            <a class="flex items-center mr-3" href="@Url.Action("Details", "Reservation", new { Id= item.Id}) "> <i data-feather="eye" class="w-4 h-4 mr-1"></i> View </a>
                                            @if (item.isCancelled == false && item.isFinished == false)
                                            {
                                                <a class="flex items-center text-theme-6 cancel_res_index" href="javascript:;" data-id="@item.Id"> <i data-feather="x" class="w-4 h-4 mr-1"></i> Cancel </a>
                                            }
                                        </div>
                                    </td>
                                </tr>
                            }
                        }
                        else
                        {
                            <tr class="intro-x">
                                <td class=" px-5 py-3 mb-3 flex items-center">
                                    <h3 class="text-center">No Reservations Yet </h3>
                                </td>
                            </tr>
                        }

                    </tbody>
                </table>
            </div>
        </div>
        <!-- END: Sales Report -->
        <!-- BEGIN: Weekly Top Seller -->
        <div class="col-span-12 sm:col-span-3 lg:col-span-3 mt-8">
            <div class="intro-y flex items-center h-10">
                <h2 class="text-lg font-medium truncate mr-5">
                  Profit Share
                </h2>
              
            </div>

            @{
                var ReservationProfit = 0;
                var SalesProfit = 0;

                foreach (g_auto.Models.Reservation item in Model.Reservations)
                {
                    ReservationProfit += Convert.ToInt32(item.Price);
                }

                foreach (g_auto.Models.Sale item in Model.Sales)
                {
                    SalesProfit += Convert.ToInt32(item.Price);
                }
            }

            <div class="intro-y box p-5 mt-5">
                <canvas class="mt-3" id="report-pie-chart" height="280"></canvas>
                <div class="mt-8">
                    <div class="flex items-center">
                        <div class="w-2 h-2 bg-theme-11 rounded-full mr-3"></div>
                        <span class="truncate">Profit From Rentals</span>
                        <div class="h-px flex-1 border border-r border-dashed border-gray-300 mx-3 xl:hidden"></div>
                        <span class="font-medium xl:ml-auto"><span id="first-half">@((ReservationProfit+SalesProfit)!=0? ReservationProfit * 100 / (ReservationProfit + SalesProfit):0)</span>%</span>
                    </div>
                    <div class="flex items-center mt-4">
                        <div class="w-2 h-2 bg-theme-1 rounded-full mr-3"></div>
                        <span class="truncate">Profit From Sales</span>
                        <div class="h-px flex-1 border border-r border-dashed border-gray-300 mx-3 xl:hidden"></div>
                        <span class="font-medium xl:ml-auto"><span id="second-half">@((ReservationProfit + SalesProfit) != 0 ? SalesProfit *100/(ReservationProfit+SalesProfit):0)</span>%</span>
                    </div>
                    <div class="flex items-center mt-4">
                        <div class="w-2 h-2 bg-theme-12 rounded-full mr-3"></div>
                        <span class="truncate">Other</span>
                        <div class="h-px flex-1 border border-r border-dashed border-gray-300 mx-3 xl:hidden"></div>
                        <span class="font-medium xl:ml-auto"><span id="third-half">1</span>%</span>
                    </div>
                </div>
            </div>
        </div>
        <!-- END: Weekly Top Seller -->
        <!-- BEGIN: Sales Report -->
        <!-- END: Sales Report -->
        <!-- BEGIN: Official Store -->
        <div class="col-span-12 xl:col-span-8 mt-6">
            <div class="intro-y block sm:flex items-center h-10">
                <h2 class="text-lg font-medium truncate mr-5">
                    Latest Sales
                </h2>

            </div>
            <div class="intro-y box p-5 mt-12 sm:mt-5">
                <table class="table table-report table-report--bordered display  w-full">
                    <thead>
                        <tr>

                            <th class="border-b-2 text-left whitespace-no-wrap">PRODUCT NAME</th>
                            <th class="border-b-2 text-center whitespace-no-wrap">DATE</th>
                            <th class="border-b-2 text-center whitespace-no-wrap">QUANTITY SOLD</th>
                            <th class="border-b-2 text-center whitespace-no-wrap">TOTAL</th>

                            <th class="border-b-2 text-center whitespace-no-wrap">STATUS</th>
                            <th class="border-b-2 text-center whitespace-no-wrap">ACTIONS</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.Sales.FirstOrDefault() != null)
                        {
                            foreach (g_auto.Models.Sale item in Model.Sales.OrderByDescending(c => c.PostDate).Take(5))
                            {
                                <tr>
                                    <td class="border-b">
                                        @if (item.Product != null)
                                        {
                                            <div class="font-medium whitespace-no-wrap">@item.Product.Name</div>
                                            <div class="text-gray-600 text-xs whitespace-no-wrap">@item.Product.ProductToCategory.FirstOrDefault().ProductCategory.Name</div>
                                        }
                                    </td>

                                    <td class="text-center border-b">@item.PostDate.ToString("MMM d")</td>
                                    <td class="text-center border-b">@item.Amount.ToString("0")</td>
                                    <td class="text-center border-b">$@item.Price.ToString("0.00")</td>
                                    <td class="w-40 border-b">
                                        @if (item.isCancelled)
                                        {
                                            <div class="flex items-center sm:justify-center text-theme-6"> <i data-feather="x-circle" class="w-4 h-4 mr-2"></i> Cancelled </div>
                                        }
                                        else if (item.isActive)
                                        {
                                            <div class="flex items-center sm:justify-center text-theme-10"> <i data-feather="activity" class="w-4 h-4 mr-2"></i> Active </div>
                                        }
                                        else if (item.isFinished)
                                        {
                                            <div class="flex items-center sm:justify-center text-theme-9"> <i data-feather="check" class="w-4 h-4 mr-2"></i> Finished </div>
                                        }
                                        else
                                        {
                                            <div class="flex items-center sm:justify-center text-theme-11"> <i data-feather="alert-circle" class="w-4 h-4 mr-2"></i> Pending </div>
                                        }

                                    </td>
                                    <td class="border-b w-5 status_container">
                                        <div class="flex sm:justify-center items-center">
                                            <a class="flex items-center mr-3" href="@Url.Action("Details", "Sale", new { Id= item.Id}) "> <i data-feather="eye" class="w-4 h-4 mr-1"></i> View </a>
                                            @if (item.isCancelled == false && item.isFinished == false)
                                            {
                                                <a class="flex items-center text-theme-6 cancel_order_index" href="" data-id="@item.Id"> <i data-feather="x" class="w-4 h-4 mr-1"></i> Cancel </a>
                                            }
                                        </div>
                                    </td>
                                </tr>
                            }

                        }
                        else
                        {
                            <tr class="intro-x">
                                <td class=" px-5 py-3 mb-3 flex items-center">
                                    <h3 class="text-center">No Orders Yet </h3>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
        <!-- END: Official Store -->
        <!-- BEGIN: Weekly Best Sellers -->
        <div class="col-span-12 xl:col-span-4 mt-6">
            <div class="intro-y flex items-center h-10">
                <h2 class="text-lg font-medium truncate mr-5">
                    Monthly Best Sellers
                </h2>
            </div>
            <div class="mt-5">

                @if (Model.Reservations.FirstOrDefault() != null)
                {
                    foreach (g_auto.Models.Reservation item in Model.Reservations)
                    {
                        if (item.PostDate.Month == DateTime.Now.Month)
                        {
                            <div class="intro-y">
                                <div class="box px-4 py-4 mb-3 flex items-center zoom-in">
                                    <div class="w-10 h-10 flex-none image-fit rounded-md overflow-hidden">
                                        <img alt="Midone Tailwind HTML Admin Template" src="~/Uploads/@item.Model.ModelImages.FirstOrDefault().Name">
                                    </div>
                                    <div class="ml-4 mr-auto">
                                        <div class="font-medium">@item.Model.Name</div>
                                        <div class="text-gray-600 text-xs">@item.PostDate.ToString("d MMMM yyyy")</div>
                                    </div>
                                    <div class="py-1 px-2 rounded-full text-xs bg-theme-9 text-white cursor-pointer font-medium">@item.Model.Reservations.Where(c => c.PostDate.Month == DateTime.Now.Month).Count() Sales</div>
                                </div>
                            </div>
                        }
                    }
                    <a href="@Url.Action("Index","Reservation")" class="intro-y w-full block text-center rounded-md py-4 border border-dotted border-theme-15 dark:border-dark-5 text-theme-16 dark:text-gray-600">View More</a>
                }
                else
                {
                    <div class="intro-x">
                        <div class="box px-5 py-3 mb-3 flex items-center zoom-in">
                            <h3 class="text-center">No reservations yet </h3>
                        </div>
                    </div>
                }
            </div>
        </div>
        <!-- END: Weekly Best Sellers -->
        <!-- BEGIN: General Report -->
        <div class="col-span-12 grid grid-cols-12 gap-6 mt-8">
            <div class="col-span-12 sm:col-span-6 xxl:col-span-3 intro-y">
                <div class="mini-report-chart box p-5 zoom-in">
                    <div class="flex items-center">
                        <div class="w-2/4 flex-none">
                            <div class="text-lg font-medium truncate">Target Sales</div>
                            <div class="text-gray-600 mt-1">300 Sales</div>
                        </div>
                        <div class="flex-none ml-auto relative">
                            <canvas id="report-donut-chart-1" width="90" height="90"></canvas>
                            <div class="font-medium absolute w-full h-full flex items-center justify-center top-0 left-0">@(Model.Sales.Where(c=>c.isFinished).Count()/300*100)%</div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-span-12 sm:col-span-6 xxl:col-span-3 intro-y">
                <div class="mini-report-chart box p-5 zoom-in">
                    <div class="flex">
                        <div class="text-lg font-medium truncate mr-3">Social Media</div>
                        <div class="py-1 px-2 rounded-full text-xs bg-gray-200 dark:bg-dark-5 text-gray-600 dark:text-gray-300 cursor-pointer ml-auto truncate">320 Followers</div>
                    </div>
                    <div class="mt-4">
                        <canvas class="simple-line-chart-1 -ml-1" height="60"></canvas>
                    </div>
                </div>
            </div>
            <div class="col-span-12 sm:col-span-6 xxl:col-span-3 intro-y">
                <div class="mini-report-chart box p-5 zoom-in">
                    <div class="flex items-center">
                        <div class="w-2/4 flex-none">
                            <div class="text-lg font-medium truncate">New Products</div>
                            <div class="text-gray-600 mt-1">@Model.Products.Where(c => c.PostDate.Month == DateTime.Now.Month).Count() Products</div>
                        </div>
                        <div class="flex-none ml-auto relative">
                            <canvas id="report-donut-chart-2" width="90" height="90"></canvas>
                            <div class="font-medium absolute w-full h-full flex items-center justify-center top-0 left-0">@(Model.Products.Where(c => c.PostDate.Month == DateTime.Now.Month).Count()/Model.Products.Count()*100)%</div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-span-12 sm:col-span-6 xxl:col-span-3 intro-y">
                <div class="mini-report-chart box p-5 zoom-in">
                    <div class="flex">
                        <div class="text-lg font-medium truncate mr-3">Posted Ads</div>
                        <div class="py-1 px-2 rounded-full text-xs bg-gray-200 dark:bg-dark-5 text-gray-600 dark:text-gray-300 cursor-pointer ml-auto truncate">180 Campaign</div>
                    </div>
                    <div class="mt-4">
                        <canvas class="simple-line-chart-1 -ml-1" height="60"></canvas>
                    </div>
                </div>
            </div>
        </div>
        <!-- END: General Report -->
        <!-- BEGIN: Weekly Top Products -->
        <div class="col-span-12 mt-6">
            <div class="intro-y block sm:flex items-center h-10">
                <h2 class="text-lg font-medium truncate mr-5">
                   Latest Appointments
                </h2>
                @*<div class="flex items-center sm:ml-auto mt-3 sm:mt-0">
                    <button class="button box flex items-center text-gray-700 dark:text-gray-300"> <i data-feather="file-text" class="hidden sm:block w-4 h-4 mr-2"></i> Export to Excel </button>
                    <button class="ml-3 button box flex items-center text-gray-700 dark:text-gray-300"> <i data-feather="file-text" class="hidden sm:block w-4 h-4 mr-2"></i> Export to PDF </button>
                </div>*@
            </div>
            <div class="intro-y box p-5 mt-12 sm:mt-5">
                <table class="table table-report table-report--bordered display  w-full">
                    <thead>
                        <tr>
                            <th class="border-b-2 text-left whitespace-no-wrap">SERVICE NAME</th>
                            <th class="border-b-2 text-center whitespace-no-wrap">DATE POSTED</th>
                            <th class="border-b-2 text-center whitespace-no-wrap">APP DATE</th>
                            <th class="border-b-2 text-center whitespace-no-wrap">APP TIME</th>
                            <th class="border-b-2 text-center whitespace-no-wrap">CUSTOMER NAME</th>
                            <th class="border-b-2 text-center whitespace-no-wrap">STATUS</th>
                            <th class="border-b-2 text-center whitespace-no-wrap">ACTIONS</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.Appointments.FirstOrDefault() != null)
                        {
                            foreach (g_auto.Models.ReservationService item in Model.Appointments.OrderByDescending(c => c.PostDate).Take(5))
                            {
                                <tr>
                                    <td class="border-b">
                                        <div class="font-medium whitespace-no-wrap">@(item.Service!=null?item.Service.Title:"*Deleted*")</div>
                                    </td>

                                    <td class="text-center border-b">@item.PostDate.ToString("MMM d, yyyy")</td>
                                    <td class="text-center border-b">@item.AppDate.ToString("MMM d, yyyy")</td>
                                    <td class="text-center border-b">@item.Time.ToString("HH:mm")</td>
                                    <td class="text-center border-b">@(item.User!=null? item.User.FullName:"*Deleted*")</td>

                                    <td class="w-40 border-b">
                                        @if (item.isCancelled)
                                        {
                                            <div class="flex items-center sm:justify-center text-theme-6 service_status"> <i data-feather="x-circle" class="w-4 h-4 mr-2"></i> Cancelled </div>
                                        }
                                        else if (item.isActive)
                                        {
                                            <div class="flex items-center sm:justify-center text-theme-10 service_status"> <i data-feather="activity" class="w-4 h-4 mr-2"></i> Active </div>
                                        }
                                        else if (item.isFinished)
                                        {
                                            <div class="flex items-center sm:justify-center text-theme-9 service_status"> <i data-feather="check" class="w-4 h-4 mr-2"></i> Finished </div>
                                        }
                                        else
                                        {
                                            <div class="flex items-center sm:justify-center text-theme-11 service_status"> <i data-feather="alert-circle" class="w-4 h-4 mr-2"></i> Pending </div>
                                        }

                                    </td>
                                    <td class="border-b w-5">
                                        <div class="flex sm:justify-center items-center status_container">

                                            @if (item.isFinished)
                                            {
                                                <span>No Actions</span>
                                            }
                                            else if (item.isCancelled)
                                            {
                                                <span>No Actions</span>

                                            }
                                            else
                                            {
                                                if (item.isActive == false && item.isCancelled == false)
                                                {
                                                    <a class="flex items-center mr-3 text-theme-9 confirm_app_service" href="javascript:;" data-id="@item.Id"> <i data-feather="check" class="w-4 h-4 mr-1"></i> Confirm</a>
                                                }
                                                if (item.isActive)
                                                {
                                                    <a class="flex items-center mr-3 text-theme-1 finish_app_service" href="javascript:;" data-id="@item.Id"> <i data-feather="check" class="w-4 h-4 mr-1"></i> Finish</a>
                                                }
                                                if (item.isCancelled == false)
                                                {
                                                    <a class="flex items-center text-theme-6 cancel_app_service" href="javascript:;" id="" data-id="@item.Id"> <i data-feather="x" class="w-4 h-4 mr-1"></i> Cancel </a>
                                                }
                                            }
                                        </div>
                                    </td>
                                </tr>
                            }
                        }
                        else
                        {
                            <tr class="intro-x">
                                <td class=" px-5 py-3 mb-3 flex items-center">
                                    <h3 class="text-center">No Appointments Yet </h3>
                                </td>
                            </tr>
                        }

                    </tbody>
                </table>
            </div>

        </div>
        <!-- END: Weekly Top Products -->
    </div>
    <div class="col-span-12 xxl:col-span-3 xxl:border-l border-theme-5 -mb-10 pb-10">
